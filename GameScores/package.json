{
  "name": "expo-starter",
  "version": "3.1.0",
  "description": "Production-ready starter for Expo (React Native) App! Powered by cli-rn, React Navigation (v6), RN UI lib, Mobx, Reanimated 2, Dark Mode, Localization, and much more.",
  "license": "MIT",
  "author": "Batyr <dev@batyr.io>",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dc": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "p:web": "expo build:web && cd web-build && vercel --prod && cd ..",
    "p:app": "expo p",
    "p:all": "yarn p:app && yarn p:web",
    "format": "prettier --check ./src",
    "format:write": "prettier --write ./src",
    "release": "dotenv release-it"
  },
  "dependencies": {
    "@kanzitelli/if-component": "^0.2.1",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "6.0.2",
    "@react-native-picker/picker": "2.1.0",
    "@react-navigation/bottom-tabs": "^6.0.9",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@react-navigation/stack": "^6.0.11",
    "date-fns": "^2.26.0",
    "expo": "^43.0.0",
    "expo-dev-client": "~0.6.3",
    "expo-localization": "~11.0.0",
    "expo-splash-screen": "~0.13.5",
    "expo-status-bar": "~1.1.0",
    "expo-updates": "~0.10.15",
    "formik": "^2.2.9",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "mobx": "^6.3.7",
    "mobx-persist-store": "^1.0.4",
    "mobx-react": "^7.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-mmkv": "1.3.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.8.0",
    "react-native-ui-lib": "^6.4.2",
    "react-native-web": "0.17.1",
    "rn-bounceable": "^1.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/config-plugins": "^4.0.7",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/i18n-js": "^3.8.2",
    "@types/lodash": "^4.14.177",
    "@types/react": "~17.0.21",
    "@types/react-dom": "~17.0.9",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/parser": "^5.4.0",
    "dotenv-cli": "^4.1.0",
    "prettier": "^2.4.1",
    "release-it": "^14.11.8",
    "typescript": "~4.3.5"
  }
}
